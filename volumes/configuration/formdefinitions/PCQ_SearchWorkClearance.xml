        <select name="zone">
            <source class="valuelist" valuelistNameRef="zone" display="description" filter="all"/>
        </select>

        <suggest name="createdBy" freeText="true">
            <source class="userlist"/>
        </suggest>

        <daterange name="dateCreated"/>
    </fieldset>

    <fieldset name="RISKASSESSMENT_SEARCH:OTHER_PROPERTIES" label="searchCriteriaOther">
        <multiselect name="lifecycleState.name" operator="match" type="list">
            <source class="lifecyclestatelist" valuelistNameRef="IBANX_RA_BP1_PROCESS"/>
