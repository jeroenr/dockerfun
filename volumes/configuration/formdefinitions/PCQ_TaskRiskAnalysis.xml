<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE forms PUBLIC "-//iBanx//Dynamic Forms 1.0//-" "http://www.ibanx-hse.com/dtds/DynamicForms.dtd">
<forms>

    <fieldset name="TASK_RISK_ANALYSIS:PROPERTIES" label="SECTION-PROPERTIES">
        <textarea name="workDescription" width="wide"/>
    </fieldset>
    <fieldset name="TASK_RISK_ANALYSIS:DETAILS" label="SECTION-DETAILS">
        <textarea name="traHouder" width="wide" maxLength="250"/>
        <textarea name="traOpstellers" width="wide" maxLength="250"/>
        <textarea name="remark" width="wide" maxLength="250"/>
    </fieldset>

    <fieldset name="TASK_RISK_ANALYSIS:PROPERTIES:VIEW" label="SECTION-PROPERTIES">
        <select name="lifecycleStateId">
            <source class="lifecyclestatelist" valuelistNameRef="TASK_RISK_ANALYSIS_LIFECYCLE"/>
        </select>
        <textarea name="workDescription" width="wide"/>
    </fieldset>

    <fieldset name="TASK_RISK_ANALYSIS:OVERVIEW" label="SECTION-OVERVIEW">
        <select name="createdBy">
            <source class="userlist"/>
        </select>
        <date name="dateCreated"/>
        <textarea name="traOpstellers"/>
        <textarea name="traHouder"/>
        <textfield name="remark" width="wide"/>
        <textarea name="workDescription" width="wide"/>
    </fieldset>

    <form name="TASK_RISK_ANALYSIS:EDIT:1">
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES" mode="view"/>
        <fieldsetref name="GENERIC_PERMIT:SECTION:SIGNATURE_USER"/>
        <fieldsetref name="GENERIC_PERMIT:SECTION:SIGNATURE_SIGN"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:WHITELISTING"/>
    </form>

    <form name="TASK_RISK_ANALYSIS:CREATE">
        <fieldsetref name="GENERIC:SECTION:FLOC"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:DETAILS"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:WHITELISTING"/>
    </form>
    <form name="TASK_RISK_ANALYSIS:PRINT">
        <fieldsetref name="TASK_RISK_ANALYSIS:OVERVIEW" mode="view"/>
    </form>

    <form name="TASK_RISK_ANALYSIS:CONFIRM">
        <fieldsetref name="TASK_RISK_ANALYSIS:OVERVIEW" mode="view"/>
    </form>

    <form name="TASK_RISK_ANALYSIS:VIEW:1">
        <fieldsetref name="TASK_RISK_ANALYSIS:OVERVIEW" mode="view"/>
    </form>

    <form name="TASK_RISK_ANALYSIS:VIEW:2">
        <fieldsetref name="TASK_RISK_ANALYSIS:OVERVIEW" mode="view"/>
    </form>
    <form name="TASK_RISK_ANALYSIS:EDIT:2">
        <fieldsetref name="GENERIC:SECTION:FLOC"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:DETAILS"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:WHITELISTING"/>
    </form>
    <form name="TASK_RISK_ANALYSIS:EDIT:2:WITH_RISK_ASSESSMENT">
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES:VIEW" mode="view"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:DETAILS"/>
        <fieldsetref name="TASK_RISK_ANALYSIS:WHITELISTING"/>
    </form>

    <form name="TASK_RISK_ANALYSIS:EDIT:3">
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES:VIEW" mode="view"/>
    </form>
    <form name="TASK_RISK_ANALYSIS:VIEW:3">
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES:VIEW" mode="view"/>
    </form>

    <form name="TASK_RISK_ANALYSIS:VIEW:4">
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES:VIEW" mode="view"/>
    </form>

    <form name="TASK_RISK_ANALYSIS:VIEW:5">
        <fieldsetref name="TASK_RISK_ANALYSIS:PROPERTIES:VIEW" mode="view"/>
    </form>
</forms>
