<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE forms PUBLIC "-//iBanx//Dynamic Forms 1.0//-" "http://www.ibanx-hse.com/dtds/DynamicForms.dtd">
<forms>

    <fieldset name="WHITEBOARD">
        <textfield name="title" maxLength="50" required="true"/>
        <select name="priority">
            <source class="valuelist" valuelistNameRef="notePriorities" display="description"/>
        </select>
        <textarea name="body" maxLength="1000" width="wide"/>
    </fieldset>

    <form name="WHITEBOARD:EDIT">
        <fieldsetref name="WHITEBOARD"/>
        <button type="submit" name="eventSubmit_doSave"/>
        <fieldsetref name="WHITEBOARD:WHITELISTING"/>
    </form>
    <form name="WHITEBOARD:CREATE">
        <fieldsetref name="WHITEBOARD"/>
        <button type="submit" name="eventSubmit_doCreate"/>
        <fieldsetref name="WHITEBOARD:WHITELISTING"/>
    </form>

</forms>
