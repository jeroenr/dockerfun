    <fieldset name="CHANGE_ACTIVITY:HIDDEN" showlegend="false">
        <hidden name="key" value="form" persist="false"/>
        <hidden name="orgContext" persist="false"/>
        <hidden name="id" persist="false"/>
        <hidden name="crObjectKey" persist="false"/>
        <hidden name="objectClassName" persist="false"/>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:HIDDEN:CREATE" showlegend="false">
        <hidden name="lifecycle" persist="false"/>
    </fieldset>

    <fieldset name="CHANGE_ACTIVITY:OVERVIEW" label="LABEL_SECTION_PROPERTIES">
        <textfield name="title" width="wide"/>

        <select name="releaseInStep" required="true">
            <source class="happyflowstatenamelist" valuelistNameRef="CHANGE_PROCESS"/>
        </select>
        <select name="finishInStep" required="true">
            <source class="happyflowstatenamelist" valuelistNameRef="CHANGE_PROCESS"/>
        </select>
        <select name="phase">
            <source class="valuelist" valuelistNameRef="phase" display="description"/>
        </select>

        <select type="radio" name="isTemplate">
            <source class="valuelist" valuelistNameRef="boolean" display="description"/>
        </select>
        <select type="radio" name="generated">
            <source class="valuelist" valuelistNameRef="boolean" display="description"/>
        </select>
        <select name="lifecycleStateId">
            <source class="lifecyclestatelist" valuelistNameRef="DEFAULT_CA_PROCESS"/>
        </select>
        <select name="assignee">
            <source class="userlist"/>
        </select>
        <date name="finishedDate"/>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:DESCRIPTION" label="LABEL_SECTION_PROPERTIES">
        <select name="lifecycleStateId" mode="view">
            <source class="lifecyclestatelist" valuelistNameRef="DEFAULT_CA_PROCESS"/>
        </select>
        <textfield maxLength="75" name="title" required="true" width="wide"/>
        <textarea maxLength="1000" name="description" width="wide"/>
        <checkbox name="mandatory"/>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:PLANNING" label="LABEL_SECTION_PLANNING">
        <date name="startDate"/>
        <date name="dueDate"/>
        <date name="finishedDate" label="endDate" mode="view"/>
        <hidden name="funcLevelOne"/>
        <suggest name="assignee" required="true" minLength="0">
            <source class="userlist"/>
        </suggest>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:PLANNING:APPROVAL" label="LABEL_SECTION_PLANNING">
        <hidden name="funcLevelOne"/>
        <suggest name="assignee" required="true" minLength="0">
            <source class="userlist"/>
        </suggest>
        <date name="finishedDate" label="endDate" mode="view"/>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:PLANNING:TEMPLATE" label="LABEL_SECTION_PLANNING">
        <suggest name="funcLevelOne" minLength="0" required="true" maxLength="50" cssClass="uppercase">
            <source class="plantlist" valuelistNameRef="funcLevelOne" display="value,description"/>
        </suggest>
        <suggest name="executedByRole" minLength="0" required="false">
            <source class="rolelist"/>
        </suggest>
        <suggest name="assignee" minLength="0">
            <source class="userlist"/>
        </suggest>
        <suggest name="orgLevelThree" required="false" minLength="0" label="orgLevelThree">
            <source class="valuelist" valuelistNameRef="departmentUser" display="description"/>
        </suggest>
        <select type="radio" name="skillRequired" required="true">
            <source class="inline">
                <option value="true" discloses="CHANGE_ACTIVITY_SKILL_SELECTION"/>
                <option value="false"/>
            </source>
        </select>
    </fieldset>

    <fieldset name="CHANGE_ACTIVITY_SKILL_SELECTION" showlegend="false">
        <suggest name="skill" required="false" minLength="0" label="userSkills">
            <source class="valuelist" valuelistNameRef="userSkills" display="description"/>
        </suggest>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:FLOW" label="LABEL_SECTION_PROPERTIES_CLASS">
        <select type="dropdown" name="releaseInStep" required="true" >
            <source class="happyflowstatenamelist" valuelistNameRef="CHANGE_PROCESS" display="showphase"/>
        </select>
        <select type="dropdown" name="finishInStep" required="true">
            <source class="happyflowstatenamelist" valuelistNameRef="CHANGE_PROCESS" display="showphase"/>
        </select>
    </fieldset>

    <fieldset name="CHANGE_ACTIVITY:EXECUTION" label="LABEL_SECTION_EXECUTION">
        <textfield maxLength="50" name="resolution"/>
        <textarea maxLength="1000" name="comment" width="wide"/>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:REASON" showlegend="false">
        <textarea maxLength="1000" name="reason" required="true" width="wide"/>
    </fieldset>
    <fieldset name="CHANGE_ACTIVITY:HIDDEN:CRREF" showlegend="false">
        <hidden name="crId"/>
    </fieldset>

    <fieldset name="CHANGE_ACTIVITY_BUDGET" label="LABEL_SECTION_BUDGET">
        <textfield name="budgetEstimated" maxLength="8" value="0" required="true" maskref="number"/>
        <textfield name="budgetInHours" maxLength="8" value="0" maskref="number" required="true"/>
        <textfield name="budgetInTime" maxLength="8" value="0" maskref="number" required="true"/>
    </fieldset>

</forms>