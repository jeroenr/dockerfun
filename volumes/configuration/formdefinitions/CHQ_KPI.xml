<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE forms PUBLIC "-//iBanx//Dynamic Forms 1.0//-" "http://www.ibanx-hse.com/dtds/DynamicForms.dtd">
<forms>
    <form name="KPI:STAT:WORKEDHOURSMONTHLY">
        <hidden name="name" value="workedHours"/>
        <hidden name="period" value="1m"/>
        <month name="startDate" label="stats_period_month" required="true"/>
        <textfield name="value" label="workedHours" maskref="decimal" required="true"/>

        <fieldsetref name="GENERIC_KPI:WHITELISTING"/>

        <button type="submit" name="eventSubmit_doSavestatistic" label="eventSubmit_doSave"/>
    </form>

    <form name="KPI:SETTINGS">
        <textfield name="yearlyWorkedHoursNormalizationConstant" maskref="decimal" required="true"/>

        <fieldsetref name="GENERIC_KPI:WHITELISTING"/>

        <button type="submit" name="eventSubmit_doSavesettings" label="eventSubmit_doSave"/>
    </form>
</forms>

