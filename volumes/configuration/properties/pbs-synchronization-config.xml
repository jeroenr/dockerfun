<!-- 
##- configuration -##
- func-level-two-separator-index: the index of the separator that marks the split for funcLevelTwo in the functional location code
- max-level: the maximum level the PBS is parsed and synchronized
- prefix: when left blank, all read functional locations are imported, when given only entries that start with the prefix will imported (f.i. ibbenburen has prefix IBB)
- contains-header: set this to true if the file import has a header (just one line), set to false when no header is given (only required for CSV synchronisation)
#####################
 -->
<!--configuration func-level-two-separator-index="1" prefix="IBB" max-level="3" contains-header="true"-->
<configuration func-level-two-separator-index="2" prefix="TR-" max-level="5" contains-header="false">
<!-- 
##- floc-entry -##
external: the position in the csv file
internal: the name the parser uses to persist the field in
##################
 -->

<!-- akzo config	
	<floc-entry external="0" internal="functionalLocation" />
	<floc-entry external="1" internal="functionalLocationDescription" />
	<floc-entry external="2" internal="validFrom" />
	<floc-entry external="3" internal="validTo" />
	<floc-entry external="4" internal="levelOne" />
	<floc-entry external="6" internal="tag" />
	<floc-entry external="7" internal="parent" />
-->

<!-- trn config -->
	<floc-entry external="0" internal="levelOne" />
	<floc-entry external="1" internal="functionalLocation" />
	<floc-entry external="2" internal="functionalLocationDescription" />
	<floc-entry external="9" internal="tag" />
<!-- trn config end -->

<!-- 
##- equipment-entry -##
external: the position in the csv file
internal: the name the parser uses to persist the field in
#######################
 -->
	<!-- 
	<equipment-entry external="0" internal="equipment" />
	<equipment-entry external="1" internal="equipmentDescription" />
	<equipment-entry external="2" internal="validFrom" />
	<equipment-entry external="3" internal="validTo" />
	<equipment-entry external="4" internal="levelOne" />
	<equipment-entry external="5" internal="functionalLocation" />
	-->
<!-- 
##- pbs-picklist-definition -##
the functionalLocation, as parsed by the floc-entry settings (or in SAP hardcoded) is parsed and tokenized by a '-' delimiter. 
the application knows which funcLevel is which from the given start and max level settings.
in the picklist-definition the dependencies are set up
###############################
 -->	
	<pbs-picklist-definition name="funcLevelTwo" />
	<pbs-picklist-definition name="funcLevelThree" dependency="funcLevelTwo"/>
	<pbs-picklist-definition name="funcLevelFour" dependency="funcLevelTwo,funcLevelThree"/>
	<!--pbs-picklist-definition name="floc" dependency="funcLevelTwo,funcLevelThree,funcLevelFour"/-->
	<!-- bitterfeld has no tag and different floc 
	<pbs-picklist-definition name="floc" dependency="funcLevelTwo,funcLevelThree,funcLevelFour,tag"/>
	-->
	
	<pbs-picklist-definition name="tag" dependency="funcLevelTwo,funcLevelThree,funcLevelFour"/>
	<!--pbs-picklist-definition name="equipment" dependency="funcLevelTwo,funcLevelThree,funcLevelFour,tag"/-->
	
</configuration>