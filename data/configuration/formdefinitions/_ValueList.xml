            <source class="valuelist" valuelistNameRef="funcLevelThree" display="value,description"/>
        </suggest>
        <space name="name"/>
        <multiselect name="logEventCategories" operator="equals">
            <source class="valuelist" valuelistNameRef="eventCategory" display="description"/>
        </multiselect>
        <space name="name"/>
        <textarea name="logEventTitle" maxLength="250" width="wide"/>
        <textarea name="logEventDescription" maxLength="250" width="wide"/>
        <space name="name"/>
        <followupheader name="followupheader"/>
        <followup name="MISC" width="wide"/>
        <followup name="IMPROVEMENT" width="wide"/>
        <followup name="WA" width="wide"/>
    </fieldset>

