        <space name="name"/>
        <select name="logEventBusinessObjectType" operator="equals">
            <source class="businessobjecttypelist" valuelistNameRef="ibanx.logbook.LogEventIF"/>
        </select>
        <space name="name"/>
        <!-- cannot restrict logEventPBSAttributeLevelThree because form generation logic expects the name on the
             restricted picklist to be logEventPBSAttributeLevelTwo, but that is of course funcLevelTwo
        -->
