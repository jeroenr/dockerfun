        </select>
        <!--textfield maskref="number" name="priority" maxLength="1" value="1" required="true" mode="view" / -->
        <select type="radio" required="true" name="priority" width="wide" mode="view">
            <source class="inline" translationPattern="changePriority.%s">
                <option value="1"/>
                <option value="2"/>
                <option value="3"/>
            </source>
        </select>
        <textfield maskref="number" name="subPriority" maxLength="10" value="0" required="true" mode="view"/>
        <space name="spacer"/>
