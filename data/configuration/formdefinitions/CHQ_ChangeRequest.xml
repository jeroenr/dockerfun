        </select>
        <textfield maskref="number" maxLength="8" name="budgetActual" value="0"/>
        <space name="name"/>
        <textfield maskref="number" maxLength="8" name="budgetPlanned" value="0"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:PLANNING:ONE" label="SECTION-PLANNING">
        <multiselect name="emergencyByPass" label="empty">
            <source class="inline" translationPattern="mocduration.%s">
                <option value="EMERGENCY" discloses="TYPE_CHANGE_GATEKEEPERONCALL"/>
            </source>
        </multiselect>
        <space name="name"/>
        <select type="radio" required="true" name="duration" label="empty">
            <source class="inline" translationPattern="mocduration.%s">
                <option value="TEMPORARY"/>
                <option value="PERMANENT"/>
            </source>
        </select>
        <space name="name"/>
        <multiselect name="inTurnAround" width="wide" label="empty">
            <source class="inline" translationPattern="mocduration.%s">
                <option value="INTURNAROUND"/>
            </source>
        </multiselect>
    </fieldset>
    <fieldset name="TYPE_CHANGE_GATEKEEPERONCALL" showlegend="false">
        <suggest name="principal" minLength="0" maxLength="50" required="true">
            <source class="userlist"/>
        </suggest>
    </fieldset>

    <fieldset name="TYPE:CHANGE:PLANNING:TWO:CREATE" showlegend="false">
        <date name="initialPlannedFinishedDate"/>
        <space name="spacer"/>
        <!--textfield maskref="number" name="priority" maxLength="1" value="1" required="true" / -->
        <select type="radio" required="true" name="priority" width="wide">
            <source class="inline" translationPattern="changePriority.%s">
                <option value="1"/>
                <option value="2"/>
                <option value="3"/>
            </source>
        </select>
        <textfield maskref="number" name="subPriority" maxLength="10" value="0" required="true"/>
        <space name="spacer"/>
        <checkbox name="gatekeeper" label="gatekeeperRequired"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:PLANNING:TWO:BEFORE_APPROVAL" showlegend="false">
        <date name="initialPlannedFinishedDate"/>
        <select required="true" name="flowId" width="wide" mode="view">
            <source class="inline" translationPattern="moctype.%s">
                <option value="small"/>
                <option value="small-with-gatekeeping"/>
                <option value="medium"/>
                <option value="medium-with-gatekeeping"/>
                <option value="medium-with-studies"/>
                <option value="large"/>
            </source>
        </select>
        <!--textfield maskref="number" name="priority" maxLength="1" value="1" required="true" / -->
        <select type="radio" required="true" name="priority" width="wide">
            <source class="inline" translationPattern="changePriority.%s">
                <option value="1"/>
                <option value="2"/>
                <option value="3"/>
            </source>
        </select>
        <textfield maskref="number" name="subPriority" maxLength="10" value="0" required="true"/>
        <space name="spacer"/>
        <textfield maskref="number" maxLength="8" name="budgetInHours" value="0" mode="view"/>
        <space name="name"/>
        <checkbox name="gatekeeper" label="gatekeeperRequired"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:PLANNING:TWO:IMPLEMENTATION" showlegend="false">
