    <fieldset name="TYPE:CHANGE:HIDDENFIELDS" showlegend="false">
        <hidden name="key" value="form" persist="false"/>
        <hidden name="objectClassName" value="ibanx.moc.ChangeRequestIF" persist="false"/>
        <hidden name="orgContext"/>
        <hidden name="businessObjectType" persist="false"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:TITLE" label="LABEL_SECTION_PROPERTIES">
        <textfield maxLength="85" name="title" required="true" width="wide"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:DESCRIPTION" showlegend="false">
        <textarea maxLength="1000" name="description" required="true" width="wide" label="problem"/>
        <suggest name="owner" label="coordinatorUserId" minLength="0" maxLength="50">
            <source class="userlist"/>
        </suggest>
    </fieldset>

    <fieldset name="TYPE:CHANGE:DESCRIPTION:NEW" showlegend="false">
        <textarea maxLength="1000" name="description" required="true" width="wide" label="problem"/>
        <suggest name="owner" label="mocHandler" minLength="0" maxLength="50">
            <source class="userlist"/>
        </suggest>
    </fieldset>

    <fieldset name="TYPE:CHANGE:DESCRIPTION:EXECUTION" showlegend="false">
        <textarea maxLength="1000" name="description" required="true" width="wide" label="problem"/>
        <suggest name="owner" label="coordinatorUserId" minLength="0" maxLength="50">
            <source class="userlist"/>
        </suggest>
        <suggest name="assignee" label="project_managerUserId" minLength="0" maxLength="50">
            <source class="userlist"/>
        </suggest>
    </fieldset>

    <fieldset name="TYPE:CHANGE:DOCUMENT" showlegend="false">
        <document qualifier="changeRequest" removable="false"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:DOCUMENT:REMOVABLE" showlegend="false">
        <document qualifier="changeRequest" removable="true"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:SOLUTION:DESCRIPTION" label="LABEL_SECTION_PROPERTIES_SOLUTION_DESCR">
        <textarea name="solution" maxLength="1000" required="true" width="wide"/>
        <textarea name="alternative" maxLength="1000" required="true" width="wide"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:CLASSIFICATION" label="LABEL_SECTION_PROPERTIES_CLASS">
        <multiselect name="largeCategory">
            <source class="valuelist" valuelistNameRef="largeCategory" display="description" order="ordering"/>
        </multiselect>
        <space name="name"/>
        <multiselect name="mediumCategory">
            <source class="valuelist" valuelistNameRef="mediumCategory" display="description" order="ordering"/>
        </multiselect>
        <space name="name"/>
        <riskmatrix name="initialrisk" sources="changePeopleSafety,changeHealth,changeEnvironment,changeAssets,changeReputation,changeSecurity" entity="change" required="true"/>
        <riskmatrix name="riskafterchange" sources="changePeopleSafety,changeHealth,changeEnvironment,changeAssets,changeReputation,changeSecurity" entity="change" required="true"/>
        <textarea name="riskScenario" maxLength="1000" required="true" width="wide"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:OTHER_USERS" showlegend="false">
        <select name="createdBy" label="initiatorUserId" mode="view">
            <source class="userlist"/>
        </select>
    </fieldset>

    <fieldset name="TYPE:CHANGE:FINANCES:CREATE" label="LABEL_SECTION_PROPERTIES_FINANCES">
        <textfield maxLength="25" name="sapArId"/>
        <textfield maskref="number" maxLength="7" name="sapId" value="0"/>
        <textfield maskref="number" maxLength="25" name="roi" value="0"/>
        <space name="name"/>
        <select type="radio" name="roiTimespan">
            <source class="valuelist" valuelistNameRef="changeRoi" display="description"/>
        </select>
    </fieldset>

    <fieldset name="TYPE:CHANGE:FINANCES:BEFORE_APPROVAL" label="LABEL_SECTION_PROPERTIES_FINANCES">
        <textfield maxLength="25" name="sapArId"/>
        <textfield maskref="number" maxLength="7" name="sapId" value="0"/>
        <textfield maskref="number" maxLength="25" name="roi" value="0"/>
        <textfield maskref="number" maxLength="8" name="budgetEstimated" value="0" mode="view"/>
        <select type="radio" name="roiTimespan">
            <source class="valuelist" valuelistNameRef="changeRoi" display="description"/>
        </select>
        <textfield maskref="number" maxLength="8" name="budgetActual" value="0"/>
        <space name="name"/>
        <textfield maskref="number" maxLength="8" name="budgetPlanned" value="0"/>
    </fieldset>

    <fieldset name="TYPE:CHANGE:FINANCES:IMPLEMENTATION" label="LABEL_SECTION_PROPERTIES_FINANCES">
        <textfield maxLength="25" name="sapArId" mode="view"/>
        <textfield maskref="number" maxLength="7" name="sapId" value="0"/>
        <textfield maskref="number" maxLength="25" name="roi" value="0" mode="view"/>
        <textfield maskref="number" maxLength="8" name="budgetEstimated" value="0" mode="view"/>
        <select type="radio" name="roiTimespan" mode="view">
