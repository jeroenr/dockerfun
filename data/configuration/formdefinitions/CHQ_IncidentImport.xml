<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE forms PUBLIC "-//iBanx//Dynamic Forms 1.0//-" "http://www.ibanx-hse.com/dtds/DynamicForms.dtd">
<forms>
    <form name="incident.ImportIncident" generatetag="false" mode="import">
        <datetime name="dateCreated"/>
        <hidden name="createdBy"/>
        <datetime name="dateModified"/>
        <hidden name="modifiedBy"/>
        <datetime name="dateArchived"/>
        <hidden name="archived"/>

        <hidden name="aggregateIdentifier"/>
        <hidden name="incidentIdentity"/>
        <hidden name="reason" />

        <hidden name="number"/>
        <hidden name="orgContext"/>
        <hidden name="reporterId"/>
        <hidden name="incidentReporterName" />
        <hidden name="assignee"/>
        <hidden name="owner"/>
        <hidden name="executingDepartment"/>
        <hidden name="funcLevelOne" />
        <hidden name="category" />
        <hidden name="confidential"/>
        <multiselect name="classification" >
            <source class="inline">
                <option value="FIRE"/>
                <option value="DAMAGE"/>
                <option value="HEALTH"/>
                <option value="SPILL"/>
            </source>
        </multiselect>
        <date name="incidentDate" value="now"/>
        <time name="incidentTime" value="now"/>
        <date name="dueDate"/>
        <hidden name="audit"/>

        <hidden name="incidentReporterDepartment"/>
        <hidden name="title" />
        <hidden name="description" />
        <hidden name="incidentMeasures" />
        <hidden name="incidentPrevention"/>
        <hidden name="witnessDescription"  />
        <hidden name="geoLevelThree" />

        <hidden name="funcLevelTwo" />
        <hidden name="funcLevelThree" />
        <hidden name="tag" />
        <hidden name="riskValue"/>
        <multiselect name="communicatedWith">
            <source class="valuelist" valuelistNameRef="communicated_with" display="description"/>
        </multiselect>
        <hidden name="work" />
        <hidden name="circumstances" />
        <hidden name="preventionMeasures" />
        <hidden name="requiredMeasures"/>
        <multiselect name="treatment" >
            <source class="valuelist" valuelistNameRef="incident_treatment" display="description"/>
        </multiselect>
        <hidden name="injuriesSeverity"/>
        <hidden name="expectedAbsence" />
        <multiselect name="injuries">
            <source class="valuelist" valuelistNameRef="incident_injuries" display="description"/>
        </multiselect>
        <multiselect name="locationOfInjuries">
            <source class="valuelist" valuelistNameRef="locationOfInjuries" display="description"/>
        </multiselect>
        <multiselect name="causeOfInjuries">
            <source class="valuelist" valuelistNameRef="cause_of_injuries" display="description"/>
        </multiselect>
        <!--<fieldsetref name="damageDetails"/>-->
        <multiselect name="typeOfDamage" >
            <source class="valuelist" valuelistNameRef="type_of_damage" display="description"/>
        </multiselect>
        <hidden name="directCauseDamage" />
        <hidden name="costOfDamage" />
        <hidden  name="downtime" />
        <hidden name="downtime_text"/>
        <!--<fieldsetref name="environmentalDetails"/>-->
        <multiselect name="typeOfEnvironmentalDamage" >
            <source class="valuelist" valuelistNameRef="type_of_environmental_damage" display="description"/>
        </multiselect>
        <hidden name="product"/>
        <hidden name="productAmount"/>
        <hidden name="directCauseEnvironment" />
        <hidden name="costOfEnvironmentalDamage"  />
        <multiselect name="typeOfFireOrExplosion" >
            <source class="valuelist" valuelistNameRef="type_of_fire_or_explosion" display="description"/>
        </multiselect>
        <hidden name="zone" />
        <hidden name="directCauseFireOrExplosion"  />
        <hidden name="findings" />
        <hidden name="causeOfIncident" />
        <hidden name="analysisRecommendations"  />
        <hidden name="state"/>

        <!-- other properties with widgets to match the type but on no other form -->
        <date name="registrationDate"/>
        <date name="supplementDate"/>
        <date name="verifiedDate"/>
        <date name="investigatedDate"/>
        <date name="analyzedAndPlannedDate"/>
        <date name="activitiesExecutedDate"/>
        <date name="evaluatedAndClosedDate"/>
        <date name="rejectedDate"/>
        <date name="dismissDate"/>
        <hidden name="cssClass"/>
        <hidden name="reporterId"/>
        <hidden name="statisticFive"/>
        <hidden name="statisticFour"/>
        <hidden name="statisticThree"/>
        <hidden name="statisticTwo"/>
        <hidden name="statisticOne"/>

        <hidden name="victims" />
        <hidden name="victims2" />
    </form>

    <form name="incident.ImportIncidentActivity" generatetag="false" mode="import">
        <datetime name="dateCreated"/>
        <hidden name="createdBy"/>
        <datetime name="dateModified"/>
        <hidden name="modifiedBy"/>
        <hidden name="orgContext"/>
        <datetime name="dateArchived"/>
        <hidden name="archived"/>

        <hidden name="aggregateIdentifier"/>
        <hidden name="incidentNumber"/>
        <hidden name="incidentId"/>
        <hidden name="number"/>

        <hidden name="state" />
        <hidden name="title" />
        <hidden name="description" />
        <hidden name="assignee"/>
        <date name="approveDate" />
        <date name="startDate" />
        <date name="dueDate"/>
        <date name="activitiesExecutedDate"/>
        <hidden name="statisticOne" />
        <hidden name="statisticTwo"  />
        <hidden name="funcLevelOne"  />
        <hidden name="orgLevelThree"/>
        <hidden name="description"  />
        <hidden name="completedComment"  />
        <hidden name="cssClass"/>
    </form>
</forms>

