        <checkbox name="copyStdPoints" value="true" checked="true"/>
        <space name="space"/>
        <checkbox name="copyAdhocActivities" value="true" checked="true"/>
        <space name="space"/>
        <checkbox name="copyPermits" value="true" checked="true"/>
    </fieldset>

    <fieldset name="ISOLATION_LIST_LOCKINFORMATION">
