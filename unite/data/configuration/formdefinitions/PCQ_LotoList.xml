        <select name="classification">
            <source class="valuelist" valuelistNameRef="isoListClassification" display="description"/>
        </select>
        <suggest name="funcLevelThree">
            <source class="valuelist" valuelistNameRef="funcLevelThree" display="value,description"/>
        </suggest>
        <textfield name="workOrderNr"/>
        <suggest name="tag">
            <source class="valuelist" valuelistNameRef="tag" display="value,description"/>
        </suggest>
        <document qualifier="isolationList"/>
    </fieldset>

    <fieldset name="ISOLATION_LIST:OVERVIEW:CLASSIFICATION:CONFIRM" label="LABEL_SECTION_PROPERTIES">
        <space name="space"/>
        <textarea name="remarks" width="wide"/>
        <suggest name="funcLevelTwo">
            <source class="valuelist" valuelistNameRef="funcLevelTwo" display="value,description"/>
        </suggest>
        <suggest name="funcLevelThree">
            <source class="valuelist" valuelistNameRef="funcLevelThree" display="value,description"/>
        </suggest>

        <textfield name="workOrderNr"/>
        <suggest name="tag">
            <source class="valuelist" valuelistNameRef="tag" display="value,description"/>
        </suggest>

        <document qualifier="isolationList"/>
    </fieldset>

    <fieldset name="ISOLATION_LIST:CLASSIFICATION:CONFIRM" showlegend="false">
        <select name="classification" required="true">
            <source class="valuelist" valuelistNameRef="isoListClassification" display="description"/>
        </select>
    </fieldset>

    <fieldset name="ISOLATION_LIST:DESCRIPTION:CONFIRM" label="remarks">
        <textarea name="remarks" width="wide"/>
    </fieldset>

    <fieldset name="ISOLATION_LIST_HIDDEN" showlegend="false">
        <hidden name="isTemplate"/>
        <hidden name="orgContext"/>
    </fieldset>

    <fieldset name="ISOLATION_LIST_SETTINGS" label="LABEL_SECTION_PROPERTIES">
        <select name="classification">
            <source class="valuelist" valuelistNameRef="isoListClassification" display="description"/>
        </select>
        <space name="space"/>
        <select name="zone">
            <source class="valuelist" valuelistNameRef="zone" display="description"/>
        </select>
        <textfield name="workOrderNr" required="true" cssClass="uppercase" maxLength="50"/>
        <daterange name="plannedIsolationDate"/>
        <textarea name="remarks" width="wide" required="true" maxLength="250"/>
        <url protocol="file" name="documentReference" maxLength="150"/>
        <document qualifier="isolationList"/>
    </fieldset>

    <fieldset name="ISOLATION_COPY_CHECKS" showlegend="false">
        <checkbox name="copyAdhocPoints" value="true" checked="true"/>
